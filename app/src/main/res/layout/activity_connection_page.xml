<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_connection_page">

    <Button
        android:id="@+id/getNetworks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/get_networks"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="278dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lon"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="430dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lat"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="479dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dir"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="530dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/getNetworks2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/get_gps"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="357dp" />

    <Button
        android:id="@+id/getNetworks3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/show_networks"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="602dp" />



</androidx.constraintlayout.widget.ConstraintLayout>